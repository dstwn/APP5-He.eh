<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    tools:context=".activity.dashboard.MainActivity">
    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_marginTop="?attr/actionBarSize"
        android:background="#00000000"
        android:theme="@style/AppTheme.AppBarOverlay"
        app:elevation="0dp">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <LinearLayout
                app:layout_collapseMode="parallax"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp">
            <TextView
                android:id="@+id/judulecuk"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="Dashboard"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:textColor="@color/colorBlack"
                android:textSize="25sp"
                android:textStyle="bold"
                app:fontFamily="@font/quicksand" />
            <TextView
                android:id="@+id/tglecuk"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="Tanggalan"
                android:textColor="#7c7c7c"
                android:textSize="15dp"
                app:fontFamily="@font/quicksand" />
        </LinearLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:orientation="horizontal"
            android:layout_marginTop="10dp"
            app:layout_collapseMode="pin"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/materialCardView2"
                android:layout_width="fill_parent"
                android:layout_marginRight="80dp"
                android:layout_height="50dp"
                app:cardBackgroundColor="@color/colorGrey"
                app:cardCornerRadius="7dp"
                app:cardElevation="0dp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="1dp">
                <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:hint="Are you fvcking looking for ?"
                    android:textColor="@color/colorBlack"
                    android:textColorHint="#c4c4c4"
                    android:textSize="14dp"
                    android:background="@null"
                    android:layout_gravity="center|start"
                    />
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/materialCardView3"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:layout_gravity="right"
                android:layout_marginEnd="0dp"
                android:layout_marginRight="0dp"
                app:cardBackgroundColor="@color/colorAccent"
                app:cardCornerRadius="7dp"
                app:cardElevation="0dp"
                app:layout_constraintEnd_toEndOf="parent">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/ic_wer"
                    android:tint="@color/colorWhite"
                    />
            </com.google.android.material.card.MaterialCardView>
        </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>
    </com.google.android.material.appbar.AppBarLayout>
    <include layout="@layout/content_main" />

